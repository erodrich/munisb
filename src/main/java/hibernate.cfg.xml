<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQL5Dialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://10.0.2.2:3306/munisanborja?useSSL=false</property>
    <property name="hibernate.connection.username">munisanborja</property>
    <property name="hibernate.connection.password">munisanborja</property>
    <property name="hibernate.show_sql">true</property>
    <property name="hibernate.format_sql">true</property>
    <property name="hibernate.current_session_context_class">thread</property>
    
    <mapping class="com.munisanborja.sys.model.entities.EstadoTramite" />
    <mapping class="com.munisanborja.sys.model.entities.EstadoViabilidad" />
    <mapping class="com.munisanborja.sys.model.entities.CostoBeneficio" />
    <mapping class="com.munisanborja.sys.model.entities.CostoEfectividad" />
    <mapping class="com.munisanborja.sys.model.entities.MontoInversionTotal" />
    <mapping class="com.munisanborja.sys.model.entities.Requerimiento" />
    <mapping class="com.munisanborja.sys.model.entities.Ubicacion" />
    <mapping class="com.munisanborja.sys.model.entities.UnidadFormuladora" />
    <mapping class="com.munisanborja.sys.model.entities.ProyectoPreInversion" />
    <mapping class="com.munisanborja.sys.model.entities.UbicacionValida" />
    <mapping class="com.munisanborja.sys.model.entities.UnidadEjecutoraValida" />
    <mapping class="com.munisanborja.sys.model.entities.InformeTecnico" />
    <mapping class="com.munisanborja.sys.model.entities.DetalleInformeTecnico" />
    <mapping class="com.munisanborja.sys.model.entities.Rubro" />
    <mapping class="com.munisanborja.sys.model.entities.RequerimientoEvaluado" />
    <mapping class="com.munisanborja.sys.model.entities.Persona" />
  </session-factory>
</hibernate-configuration>
